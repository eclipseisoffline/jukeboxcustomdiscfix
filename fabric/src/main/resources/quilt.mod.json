{
  "schema_version": 1,
  "quilt_loader": {
    "group": "${group}",
    "id": "jukeboxcustomdiscfix",
    "version": "${version}",
    "metadata": {
      "name": "Jukebox Custom Disc Fix",
      "description": "This mod fixes bug MC-260346, which causes custom music discs, when played in a jukebox, to be cut off when the vanilla disc would normally end.",
      "license": "GPL-3.0-only",
      "contributors": {
        "eclipseisoffline": "Author"
      },
      "contact": {
        "homepage": "https://modrinth.com/mod/jukebox-custom-disc-fix",
        "issues": "https://github.com/eclipseisoffline/jukeboxcustomdiscfix/issues",
        "sources": "https://github.com/eclipseisoffline/jukeboxcustomdiscfix"
      },
      "icon": "assets/jukeboxcustomdiscfix/icon.png"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {},
    "depends": [
      {
        "id": "quilt_loader",
        "versions": ">=${quilt_loader_version}"
      },
      {
        "id": "minecraft",
        "versions": "${supported_versions}"
      }
    ]
  },
  "mixin": "jukeboxcustomdiscfix.mixins.json"
}
