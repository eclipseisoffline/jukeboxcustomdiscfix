{
  "schemaVersion": 1,
  "id": "jukeboxcustomdiscfix",
  "version": "${version}",
  "name": "Jukebox Custom Disc Fix",
  "description": "This mod fixes bug MC-260346 for discs modified using a resource pack, which causes custom music discs, when played in a jukebox, to be cut off when the vanilla disc would normally end.",
  "authors": [
    {
      "name": "eclipseisoffline",
      "contact": {
        "homepage": "https://github.com/eclipseisoffline"
      }
    }
  ],
  "contact": {
    "homepage": "https://modrinth.com/mod/jukebox-custom-disc-fix",
    "issues": "https://github.com/eclipseisoffline/jukeboxcustomdiscfix/issues",
    "sources": "https://github.com/eclipseisoffline/jukeboxcustomdiscfix"
  },
  "license": "LGPL-3.0-only",
  "icon": "assets/jukeboxcustomdiscfix/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [],
    "main": []
  },
  "mixins": [
    "jukeboxcustomdiscfix.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "minecraft": "${supported_versions}"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.donate": "https://buymeacoffee.com/eclipseisoffline",
        "modmenu.buymeacoffee": "https://buymeacoffee.com/eclipseisoffline",
        "modmenu.modrinth": "https://modrinth.com/mod/jukebox-custom-disc-fix"
      }
    }
  }
}
